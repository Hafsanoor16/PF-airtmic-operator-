#include <iostream>

using namespace std;

int main(){

    cout<<"Task 3 inlabl"<<endl;
    int days;
    cout<<"Enter number of days"<<endl;
    cin>>days;
    int years=days/365;
    days%=365;
    int weeks=days/7;
    days%=7;

        cout<<"Equivalent"<<years<<"years"<<weeks<<"weeks"<<days<<"days"<<endl;

    return 0;
}
