    cout << "My phone number is"<<"03001234567"<<endl;
    cout <<"My birthday is on"<<"May 15,1999"<<endl;
    cout <<"My age is"<<"18"<<endl;
    cout <<"My father name is"<<"Arshad"<<endl;
    cout <<"My mother name is"<<"Iffat"<<endl;
    cout <<"My brother name is"<<"Zeeshan"<<endl;
